# Give priority to Homebrew's bin & sbin dirs on top of the system's dirs.
PATH="/usr/local/bin:/usr/local/sbin:$PATH" 
  
# Add `~/bin` to the `$PATH`
PATH="$HOME/bin:$PATH"
PATH="$HOME/local/bin:$PATH"

# Add the npm bin folder
PATH="/usr/local/share/npm/bin:$PATH"

# add z.sh bin folder
PATH="$ZSH/bin:$PATH"

export PATH

export MANPATH="/usr/local/man:/usr/local/mysql/man:/usr/local/git/man:$MANPATH"